@page "/Error/{statusCode:int}"
@model AceJobAgency.Pages.HttpErrorModel
@{
    ViewData["Title"] = Model.Title;
}

<section class="card-shell">
    <h1>@Model.Title</h1>
    <p class="muted">Status code: @Model.HttpStatusCode</p>
    <p>@Model.Message</p>
    <a asp-page="/Index" class="btn btn-primary">Back to Home</a>
</section>
